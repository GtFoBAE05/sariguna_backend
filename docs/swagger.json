                    "422": {
                        "description": "invalid payload",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorResponseJson"
                        }
                    }
                }
            }
        },
        "/user/register": {
            "post": {
                "description": "Register user",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
